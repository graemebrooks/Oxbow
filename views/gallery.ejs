<%- include('./partials/header') %>

<h1 class="title">My Gallery</h1>

<div class="gallery-container">
        <% critiques.forEach(function(crit) { %>

            <div class="card gallery-card" style="background-image: url('<%=crit.artworkImage%>');">
                    <div class="card-body">
                      <h5 class="card-title"><%= crit.artworkTitle %></h5>
                      <span class="stars">
                        <% for (let i = 0; i < crit.critiqueRating; i++) {%>
                        <i class="fas fa-star"></i>
                        <%}%>
                      </span>
                      <br>
                      <a href="/critiques/<%= crit._id %>" id="see-more-btn" class="btn btn-primary-outline rounded-0">See More</a>
                    </div>
            </div>
        
        <%  }) %>
        
</div>


 

<%- include('./partials/footer') %>